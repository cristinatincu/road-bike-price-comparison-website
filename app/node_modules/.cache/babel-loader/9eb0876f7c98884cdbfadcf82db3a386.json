    </a>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport {EventBus} from '@/event-bus';\r\n\r\nexport default {\r\n  name: \"SearchGrid\",\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      roadBikes: []\r\n    }\r\n  },\r\n  methods: {\r\n    getRoadBikes: function () {\r\n      axios.get('/api/bikes', {\r\n        params:\r\n            {\r\n              'numItems': 24,\r\n              'offset': 0,\r\n              'search': this.searchQuery\r\n            }\r\n      })\r\n          .then(response => (this.roadBikes = response.data.roadBikes))\r\n          .catch(error => console.log(error))\r\n    }\r\n  },\r\n  mounted() {\r\n    EventBus.$on('searchClicked', searchQuery => {\r\n      console.log(`Searching for ${searchQuery}`);\r\n      this.searchQuery = searchQuery;\r\n      this.getRoadBikes();\r\n    });\r\n\r\n    this.getRoadBikes(this.searchQuery);\r\n    //setInterval(this.getAllRoadBikes, 5000);\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#resultGrid {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr;\r\n  grid-gap: 2em;\r\n}\r\n\r\n.gridItem {\r\n  display: flex;\r\n  border: 2px solid #D4D4D4;\r\n  padding: 10px;\r\n}\r\n\r\n.gridItem a:link, .gridItem a:visited {\r\n  justify-content: center;\r\n  color: #FAD946;\r\n  text-decoration: none;\r\n  text-align: center;\r\n  word-spacing: 0.2em;\r\n}\r\n\r\n.gridItem img {\r\n  width: 20em;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/pages"}]}